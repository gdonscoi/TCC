<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.uniararas.actvity"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="9" />

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="br.com.uniararas.actvity.LoginActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="br.com.uniararas.actvity.MenuActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_menu" >
        </activity>
        <activity
            android:name="br.com.uniararas.actvity.ConsultaNotasActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_lista" >
        </activity>
        <activity
            android:name="br.com.uniararas.actvity.ConsultaFaltasActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_lista" >
        </activity>
        <activity
            android:name="br.com.uniararas.actvity.ConsultaAnoSemestreActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_consulta_ano_semestre" >
        </activity>
        <activity
            android:name="br.com.uniararas.actvity.SobreActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_sobre" >
        </activity>
        <activity
            android:name="br.com.uniararas.actvity.ErroActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_erro" >
        </activity>
        <activity
            android:name="br.com.uniararas.actvity.InformacoesActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_informacoes" >
        </activity>
        <activity
            android:name="br.com.uniararas.actvity.LocalizacaoActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_localizacao" >
        </activity>
        <activity
            android:name="br.com.uniararas.actvity.HistoriaActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_historia" >
        </activity>
    </application>

</manifest>
